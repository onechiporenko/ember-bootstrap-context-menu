{{#if @item.faIcon}}
  <this.FaIcon
    @icon={{@item.faIcon.icon}}
    @prefix={{@item.faIcon.prefix}}
    @size={{@item.faIcon.size}}
    @rotation={{@item.faIcon.rotation}}
    @flip={{@item.faIcon.flip}}
    @spin={{@item.faIcon.spin}}
    @transform={{@item.faIcon.transform}}
    @mask={{@item.faIcon.mask}}
    @symbol={{@item.faIcon.symbol}}
    @fixedWidth={{@item.faIcon.fixedWidth}}
  />
{{/if}}
{{@item.title}}
{{yield}}