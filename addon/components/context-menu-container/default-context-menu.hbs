<ContextMenuWrapper
  @settings={{@settings}}
  @onMenuItemClick={{@onMenuItemClick}}
  @onCloseMenu={{@onCloseMenu}} as |dd menuItemClicked|>
  <dd.menu as |ddm|>
    <ContextMenuContainer::DefaultContextMenu::List
      @ddm={{ddm}}
      @menuItemClicked={{menuItemClicked}}
      @items={{@settings.menuItems}} />
  </dd.menu>
</ContextMenuWrapper>
{{yield}}